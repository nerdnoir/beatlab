{
  "name": "beatlab",
  "version": "0.1.0-alpha",
  "description": "Compose talks and workshops from an inventory of slides and modules.",
  "engines": {
    "node": ">=7.7.0"
  },
  "main": "./lib/exports.js",
  "bin": "./bin/beatlab",
  "repository": {
    "type": "git",
    "url": "http://github.com/nerdnoir/beatlab.git"
  },
  "scripts": {
    "start": "node server.js",
    "test": "./node_modules/.bin/mocha --reporter landing --ui bdd --require ./test/helper.js ./test/**/*.tests.js",
    "spec": "./node_modules/.bin/mocha --reporter spec --ui bdd --require ./test/helper.js ./test/**/*.tests.js",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --require ./test/helper.js ./test/**/*.tests.js",
    "deploy-manual": "cd ./docs/manual/ && now && now alias"
  },
  "author": "David Laribee <david@nerdnoir.com> (https://nerdnoir.com/)",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "datauri": "^1.0.5",
    "handlebars": "^4.0.6",
    "js-yaml": "^3.8.2",
    "ramda": "^0.23.0",
    "shelljs": "^0.7.7"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-string": "^1.3.0",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0"
  }
}
